    "initial_velocity": {
      "$id": "#/properties/initial_velocity",
      "type": "integer",
      "title": "The Initial_velocity Schema",
      "default": 0,
      "examples": [
        10
      ]
    },
    "public": {
      "$id": "#/properties/public",
      "type": "boolean",
      "title": "The Public Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "atom_enabled": {
      "$id": "#/properties/atom_enabled",
      "type": "boolean",
      "title": "The Atom_enabled Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "project_type": {
      "$id": "#/properties/project_type",
      "type": "string",
      "title": "The Project_type Schema",
      "default": "",
      "examples": [
        "private"
      ],
      "pattern": "^(.*)$"
    },
    "start_time": {
      "$id": "#/properties/start_time",
      "type": "string",
      "title": "The Start_time Schema",
      "default": "",
      "examples": [
        "2019-01-28T08:00:00Z"
      ],
      "pattern": "^(.*)$"
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The Created_at Schema",
      "default": "",
      "examples": [
        "2019-01-31T12:03:09Z"
      ],
      "pattern": "^(.*)$"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The Updated_at Schema",
      "default": "",
      "examples": [
        "2019-01-31T13:01:21Z"
      ],
      "pattern": "^(.*)$"
    },
    "account_id": {
      "$id": "#/properties/account_id",
      "type": "integer",
      "title": "The Account_id Schema",
      "default": 0,
      "examples": [
        1080258
      ]
    },
    "current_iteration_number": {
      "$id": "#/properties/current_iteration_number",
      "type": "integer",
      "title": "The Current_iteration_number Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "enable_following": {
      "$id": "#/properties/enable_following",
      "type": "boolean",
      "title": "The Enable_following Schema",
      "default": false,
      "examples": [
        true
      ]
    }
  }
