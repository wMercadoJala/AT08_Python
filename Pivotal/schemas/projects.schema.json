{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "account_id",
    "atom_enabled",
    "automatic_planning",
    "bugs_and_chores_are_estimatable",
    "created_at",
    "current_iteration_number",
    "description",
    "enable_following",
    "enable_incoming_emails",
    "enable_tasks",
    "has_google_domain",
    "id",
    "initial_velocity",
    "iteration_length",
    "kind",
    "name",
    "number_of_done_iterations_to_show",
    "point_scale",
    "point_scale_is_custom",
    "profile_content",
    "project_type",
    "public",
    "start_date",
    "start_time",
    "time_zone",
    "updated_at",
    "velocity_averaged_over",
    "version",
    "week_start_day"
  ],
  "properties": {
    "account_id": {
      "$id": "#/properties/account_id",
      "type": "integer",
      "title": "The Account_id Schema",
      "default": 0,
      "examples": [
        100
      ]
    },
    "atom_enabled": {
      "$id": "#/properties/atom_enabled",
      "type": "boolean",
      "title": "The Atom_enabled Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "automatic_planning": {
      "$id": "#/properties/automatic_planning",
      "type": "boolean",
      "title": "The Automatic_planning Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "bugs_and_chores_are_estimatable": {
      "$id": "#/properties/bugs_and_chores_are_estimatable",
      "type": "boolean",
      "title": "The Bugs_and_chores_are_estimable Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The Created_at Schema",
      "default": "",
      "examples": [
        "2019-01-29T12:00:05Z"
      ],
      "pattern": "^(.*)$"
    },
    "current_iteration_number": {
      "$id": "#/properties/current_iteration_number",
      "type": "integer",
      "title": "The Current_iteration_number Schema",
      "default": 0,
      "examples": [
        15
      ]
    },
    "enable_following": {
      "$id": "#/properties/enable_following",
      "type": "boolean",
      "title": "The Enable_following Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "enable_incoming_emails": {
      "$id": "#/properties/enable_incoming_emails",
      "type": "boolean",
      "title": "The Enable_incoming_emails Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "enable_tasks": {
      "$id": "#/properties/enable_tasks",
      "type": "boolean",
      "title": "The Enable_tasks Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "has_google_domain": {
      "$id": "#/properties/has_google_domain",
      "type": "boolean",
      "title": "The Has_google_domain Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        99
      ]
    },
    "initial_velocity": {
      "$id": "#/properties/initial_velocity",
      "type": "integer",
      "title": "The Initial_velocity Schema",
      "default": 0,
      "examples": [
        10
      ]
    },
    "iteration_length": {
      "$id": "#/properties/iteration_length",
      "type": "integer",
      "title": "The Iteration_length Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "kind": {
      "$id": "#/properties/kind",
      "type": "string",
      "title": "The Kind Schema",
      "default": "",
      "examples": [
        "project"
      ],
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Death Star"
      ],
      "pattern": "^(.*)$"
    },
    "number_of_done_iterations_to_show": {
      "$id": "#/properties/number_of_done_iterations_to_show",
      "type": "integer",
      "title": "The Number_of_done_iterations_to_show Schema",
      "default": 0,
      "examples": [
        4
      ]
    },
    "point_scale": {
      "$id": "#/properties/point_scale",
      "type": "string",
      "title": "The Point_scale Schema",
      "default": "",
      "examples": [
        "0,1,2,3"
      ],
      "pattern": "^(.*)$"
    },
    "point_scale_is_custom": {
      "$id": "#/properties/point_scale_is_custom",
      "type": "boolean",
      "title": "The Point_scale_is_custom Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "profile_content": {
      "$id": "#/properties/profile_content",
      "type": "string",
      "title": "The Profile_content Schema",
      "default": "",
      "examples": [
        "This is a machine of war such as the universe has never known. It's colossal, the size of a class-four moon. And it possesses firepower unequaled in the history of warfare."
      ],
      "pattern": "^(.*)$"
    },
    "project_type": {
      "$id": "#/properties/project_type",
      "type": "string",
      "title": "The Project_type Schema",
      "default": "",
      "examples": [
        "private"
      ],
      "pattern": "^(.*)$"
    },
    "public": {
      "$id": "#/properties/public",
      "type": "boolean",
      "title": "The Public Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "start_date": {
      "$id": "#/properties/start_date",
      "type": "string",
      "title": "The Start_date Schema",
      "default": "",
      "examples": [
        "2018-10-15"
      ],
      "pattern": "^(.*)$"
    },
    "start_time": {
      "$id": "#/properties/start_time",
      "type": "string",
      "title": "The Start_time Schema",
      "default": "",
      "examples": [
        "2019-01-29T12:00:10Z"
      ],
      "pattern": "^(.*)$"
    },
    "time_zone": {
      "$id": "#/properties/time_zone",
      "type": "object",
      "title": "The Time_zone Schema",
      "required": [
        "kind",
        "olson_name",
        "offset"
      ],
      "properties": {
        "kind": {
          "$id": "#/properties/time_zone/properties/kind",
          "type": "string",
          "title": "The Kind Schema",
          "default": "",
          "examples": [
            "time_zone"
          ],
          "pattern": "^(.*)$"
        },
        "olson_name": {
          "$id": "#/properties/time_zone/properties/olson_name",
          "type": "string",
          "title": "The Olson_name Schema",
          "default": "",
          "examples": [
            "America/Los_Angeles"
          ],
          "pattern": "^(.*)$"
        },
        "offset": {
          "$id": "#/properties/time_zone/properties/offset",
          "type": "string",
          "title": "The Offset Schema",
          "default": "",
          "examples": [
            "-08:00"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The Updated_at Schema",
      "default": "",
      "examples": [
        "2019-01-29T12:00:10Z"
      ],
      "pattern": "^(.*)$"
    },
    "velocity_averaged_over": {
      "$id": "#/properties/velocity_averaged_over",
      "type": "integer",
      "title": "The Velocity_averaged_over Schema",
      "default": 0,
      "examples": [
        3
      ]
    },
    "version": {
      "$id": "#/properties/version",
      "type": "integer",
      "title": "The Version Schema",
      "default": 0,
      "examples": [
        66
      ]
    },
    "week_start_day": {
      "$id": "#/properties/week_start_day",
      "type": "string",
      "title": "The Week_start_day Schema",
      "default": "",
      "examples": [
        "Monday"
      ],
      "pattern": "^(.*)$"
    }
  }
}